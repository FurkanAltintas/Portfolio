@using Portfolio.Web.Helpers;
@model IDataResult<List<MenuGetAllDto>>

@await Component.InvokeAsync("LeftSideBar")

@if (Model.IsSuccess) // İçerisinde data var ise
{
    foreach (var menu in Model.Data) // var olan dataları dönüyoruz
    {
        if (HasViewComponentHelper.Init(menu.ComponentName)) // database de olan datalar progrram içerisinde var mı ?
        {
            @await Component.InvokeAsync(menu.ComponentName);
        }
    }
}